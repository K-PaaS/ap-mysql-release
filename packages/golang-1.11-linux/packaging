set -e -x -u

tar xzf golang-1.11-linux/go*.linux-amd64.tar.gz

cp -R go/* ${BOSH_INSTALL_TARGET}

mkdir ${BOSH_INSTALL_TARGET}/bosh
cp golang-1.11-linux/runtime.env.linux   ${BOSH_INSTALL_TARGET}/bosh/runtime.env
cp golang-1.11-linux/compile.env.linux   ${BOSH_INSTALL_TARGET}/bosh/compile.env
cp golang-1.11-linux/compile.env.generic ${BOSH_INSTALL_TARGET}/bosh/compile.env.generic
